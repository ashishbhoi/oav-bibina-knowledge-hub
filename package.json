{
  "name": "oav-bibina-knowledge-hub",
  "private": true,
  "version": "1.0.3",
  "description": "Knowledge hub for OAV Bibina",
  "author": "Ashish Kumar Bhoi",
  "license": "GPL-3.0-only",
  "keywords": [
    "sveltekit",
    "cloudflare",
    "tailwindcss",
    "typescript",
    "pnpm",
    "Education",
    "Knowledge Hub"
  ],
  "type": "module",
  "scripts": {
    "dev": "pnpm run build && wrangler dev",
    "dev:vite": "vite dev",
    "build": "vite build",
    "preview": "pnpm run build && wrangler dev",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "deploy": "pnpm run build && wrangler deploy",
    "cf-typegen": "wrangler types ./src/worker-configuration.d.ts"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^6.0.0",
    "@sveltejs/adapter-cloudflare": "^7.2.3",
    "@sveltejs/kit": "^2.22.0",
    "@sveltejs/vite-plugin-svelte": "^6.0.0",
    "@types/node": "^24.3.1",
    "@types/uuid": "^10.0.0",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.0.0",
    "vite": "^7.0.4",
    "wrangler": "^4.34.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.883.0",
    "@aws-sdk/s3-request-presigner": "^3.883.0",
    "bcryptjs": "^3.0.2",
    "jose": "^6.1.0",
    "uuid": "^12.0.0",
    "zod": "^4.1.5"
  }
}
